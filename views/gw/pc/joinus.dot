[[##scriptSection
	<script type="text/javascript">
	$(function(){
		$('.choice-block p').click(function(){
			var id = '#' + $(this).attr('data-type');
			$('.choice-block p').removeClass('on');
			$(this).addClass('on');
			$('.part').hide();
			$(id).show();
		});
		
		$('.top').click(function(){
			var open = $(this).find('.js-upload').attr('data-open');			
			if(open == 1){
				$(this).find('.js-upload').attr('src','/gw/pc/images/joinus/icn_down.png');
				$(this).find('.js-upload').attr('data-open','0');
				$(this).parents('.type').find('.zjDetail').hide();
				$(this).parents('.type').find('.line-block').show();
			}else if(open == 0){
				$(this).find('.js-upload').attr('src','/gw/pc/images/joinus/icn_up.png');
				$(this).find('.js-upload').attr('data-open','1');
				$(this).parents('.type').find('.zjDetail').show();
				$(this).parents('.type').find('.line-block').hide();
			}
		});
	});
	</script>
