[[##scriptSection
	<script type="text/javascript">
	$(function(){
		$('.choice-block p').click(function(){
			var id = '#' + $(this).attr('data-type');
			$('.choice-block p').removeClass('on');
			$(this).addClass('on');
			$('.part').hide();
			$(id).show();
		});

		$('#clicktorule').click(function(){
			$('.feidoumask').show();
		});

		$('#feidou-close').click(function(){
			$('.feidoumask').hide();
		});
	});
	</script>
